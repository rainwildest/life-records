type Budget {
  id: ID!
  seqId: Int
  userId: String
  expenseId: String
  amounts: Int
  createdAt: Date
  modifiedAt: Date
  deletedAt: Date
}

input ModifyBudgetOptions {
  expenseId: String
  amounts: Int
}

type Mutation {
  createBudget(input: ModifyBudgetOptions): Budget
  modifyBudget(id: String!, input: ModifyBudgetOptions): Budget
  removeBudget(id: String!): Budget
}
