type FundPlan {
  id: ID!
  seqId: Int
  expenseId: String
  userId: String
  amounts: Int
  approximateAt: Date
  createdAt: Date
  modifiedAt: Date
  deletedAt: Date
}

type fieldFundPlanOptions {
  amounts: Int
  expenseId: String
  approximateAt: Date
}

type Mutation {
  createFundPlan(input: fieldFundPlanOptions): FundPlan
  modifyFundPlan(id: String, input: fieldFundPlanOptions): FundPlan
  removeFundPlan(id: String!): FundPlan
}
