type Query {
  accountBooks: [AccountBooks]
}

type Mutation {
  createAccountBooks(name: String!): AccountBooks
  modifyAccountBooks(id: String!, name: String!): AccountBooks
  removeAccountBooks(id: String!): AccountBooks
}
