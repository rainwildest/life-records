type User {
  id: ID!
  seqId: Int
  username: String
  birthday: String
  gender: String
  email: String
  githubProviderId: String
  googleProviderId: String
  phoneNumber: String
  password: String
  avatar: String
  isAdmin: Boolean
  isVerify: Boolean
  createdAt: Date
  modifiedAt: Date
  deletedAt: Date
}

type Query {
  currentUser: User
}
